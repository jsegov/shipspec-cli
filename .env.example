# OpenRouter API Key (required for cloud models)
OPENROUTER_API_KEY=your_openrouter_api_key_here

# Tavily API Key (required for web search)
TAVILY_API_KEY=your_tavily_api_key_here

# Ollama Base URL (optional, defaults to http://localhost:11434)
OLLAMA_BASE_URL=http://localhost:11434

# Environment Mode
NODE_ENV=development

# Ship Spec Configuration Control
SHIPSPEC_LOAD_DOTENV=1
SHIPSPEC_DOTENV_OVERRIDE=0
SHIPSPEC_DOTENV_OVERRIDE_ACK=
SHIPSPEC_DOTENV_PATH=
SHIPSPEC_STRICT_CONFIG=0
SHIPSPEC_DEBUG_DIAGNOSTICS=0
SHIPSPEC_DEBUG_DIAGNOSTICS_ACK=
ALLOW_LOCALHOST_LLM=0

# LangSmith Configuration Control
LANGSMITH_API_KEY=your_langsmith_api_key_here